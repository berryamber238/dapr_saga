{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Dapr": {
    "PubSub": "pubsub"
  },
  "nacos": {
    "ServerAddresses": [ "http://localhost:8848" ],
    "DefaultTimeOut": 15000,
    "Namespace": "public",
    "ListenInterval": 1000,
    "ServiceName": "service-outbox-worker",
    "Listeners": [
      {
        "Optional": true,
        "DataId": "service-outbox-worker.json",
        "Group": "DEFAULT_GROUP"
      },
      {
        "Optional": true,
        "DataId": "shared-config.json",
        "Group": "DEFAULT_GROUP"
      }
    ]
  }
}
