{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "DaprDemoDB"
  },
  "Dapr": {
    "StateStore": "statestore",
    "PubSub": "pubsub"
  },
  "RetryPolicy": {
    "MaxRetries": 3,
    "InitialDelayMilliseconds": 1000
  }
}
